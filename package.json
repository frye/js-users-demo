{
  "name": "github-dx-demo",
  "version": "1.0.2",
  "description": "Demo repository highlighting an integrated developer experience with GitHub ecosystem tools.",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "test": "jest --coverage",
    "build": "ncc build src/index.js --license licenses.txt"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collinmcneese/github-dx-demo.git"
  },
  "keywords": [
    "github",
    "developer-experience"
  ],
  "author": "Collin McNeese <collinmcneese@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/collinmcneese/github-dx-demo/issues"
  },
  "homepage": "https://github.com/collinmcneese/github-dx-demo#readme",
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "cspell": "^6.31.1",
    "eslint": "^8.39.0",
    "eslint-config-strongloop": "^2.1.0",
    "jest": "^30.2.0",
    "markdownlint-cli": "^0.33.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "express": "^4.21.1",
    "request": "^2.88.2",
    "sequelize": "6.29.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "yamljs": "^0.3.0"
  }
}
